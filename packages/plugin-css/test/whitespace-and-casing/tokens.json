{
  "my colors": {
    "$type": "color",
    "blue": {
      "$value": "#0000ff"
    },
    " red": {
      "$value": "#ff0000"
    },
    "a nice green": {
      "$value": "#ff0000"
    },
    " black": {
      "$value": "#000000"
    },
    "primary ": {
      "$value": "{my colors.a nice green}"
    }
  },
  "Pretty Typography": {
    "family": {
      "body": {"$type": "fontFamily", "$value": ["IBM Plex Sans", "-system-ui", "sans-serif"]},
      "footnote": {"$type": "fontFamily", "$value": "{Pretty Typography.family.body}"},
      "heading": {"$type": "fontFamily", "$value": "Helvetica"}
    },
    "page-title": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{Pretty Typography.family.heading}",
        "fontSize": "48px",
        "fontWeight": 600,
        "lineHeight": 1.25,
        "letterSpacing": "0.125em"
      }
    },
    "Subheading": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{Pretty Typography.family.heading}",
        "fontSize": "18px",
        "fontWeight": 500,
        "lineHeight": 1.375,
        "letterSpacing": "0.25em",
        "textTransform": "uppercase"
      }
    },
    "Body": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{Pretty Typography.family.body}",
        "fontSize": "16px",
        "fontWeight": 400,
        "lineHeight": 1.5
      }
    }
  }
}
