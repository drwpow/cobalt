{
  "color": {
    "$type": "color",
    "gray": {
      "$value": "#544741"
    },
    "yellow": {
      "$value": "#544741"
    }
  },
  "border": {
    "$type": "border",
    "gray": {
      "$value": {
        "color": "{color.gray}",
        "width": "1px",
        "style": "solid"
      }
    },
    "warning": {
      "$value": {
        "color": "{color.yellow}",
        "width": "2px",
        "style": "solid"
      }
    }
  }
}
