{
  "$name": "Cobalt",
  "color": {
    "$type": "color",
    "black": {
      "$value": "#00193f"
    },
    "blue": {
      "00":  { "$value": "{color.black}" },
      "10":  { "$value": "#062053" },
      "20":  { "$value": "#0f2868" },
      "30":  { "$value": "#192f7d" },
      "40":  { "$value": "#223793" },
      "50":  { "$value": "#2b3faa" },
      "60":  { "$value": "#3764ba" },
      "70":  { "$value": "#4887c9" },
      "80":  { "$value": "#5ca9d7" },
      "90":  { "$value": "#72cce5" },
      "100": { "$value": "#89eff1" }
    },
    "darkGray":  { "$value": "#282a37" },
    "green":     { "$value": "#77cd8f" },
    "purple":    { "$value": "#6a35d9" },
    "red":       { "$value": "#f6566a" },
    "white":     { "$value": "#ffffff" },
    "code": {
      "bg": {
        "greenShift": { "$value": "#2b5eaa" }
      },
      "gold":    { "$value": "#ffdc2b" },
      "green":   { "$value": "#79ff6a" },
      "orange":  { "$value": "#ff6418" },
      "red":     { "$value": "#f56d65" },
      "pink":    { "$value": "#ff5ae2" },
      "skyBlue": { "$value": "#59c1ff" }
    }
  },
  "ui": {
    "bg": {
      "$type": "color",
      "$value": "{color.white}",
      "$extensions": {
        "mode": {
          "light": "{color.white}",
          "dark": "#001824"
        }
      }
    },
    "fg": {
      "$type": "color",
      "$value": "{color.black}",
      "$extensions": {
        "mode": {
          "light": "{color.black}",
          "dark": "{color.white}"
        }
      }
    },
    "action": {
      "$type": "color",
      "$value": "{color.blue.50}",
      "$extensions": {
        "mode": {
          "light": "{color.blue.50}",
          "dark": "{color.blue.80}"
        }
      }
    }
  },
  "font": {
    "family": {
      "$type": "fontFamily",
      "ppNeue": { "$value": ["PP Neue Montreal", "system-ui", "ui-sans-serif", "-apple-system", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"] },
      "mono":   { "$value": ["Red Hat Mono", "ui-monospace", "SF Mono", "Monaco", "Cascadia Mono", "Cascadia Code", "Consolas", "monospace"] }
    },
    "size": {
      "$type": "dimension",
      "01": { "$value": "8px" },
      "02": { "$value": "9px" },
      "03": { "$value": "12px" },
      "04": { "$value": "14px" },
      "05": { "$value": "18px" },
      "06": { "$value": "22px" },
      "07": { "$value": "28px" },
      "08": { "$value": "32px" }
    }
  },
  "space": {
    "$description": "IBM Carbon spacing",
    "$type": "dimension",
    "4xs": { "$value": "1px" },
    "3xs": { "$value": "2px" },
    "2xs": { "$value": "4px" },
    "xs": { "$value": "8px" },
    "sm": { "$value": "12px" },
    "md": { "$value": "16px" },
    "lg": { "$value": "24px" },
    "xl": { "$value": "32px" },
    "2xl": { "$value": "40px" },
    "3xl": { "$value": "48px" },
    "layout": {
      "2xs": { "$value": "16px" },
      "xs": { "$value": "24px" },
      "sm": { "$value": "32px" },
      "md": { "$value": "48px" },
      "lg": { "$value": "64px" },
      "xl": { "$value": "96px" },
      "2xl": { "$value": "160px" }
    }
  },
  "ease": {
    "$type": "cubicBezier",
    "linear": { "$value": [0, 0, 1, 1] },
    "inCirc": { "$value": [0.55, 0, 1, 0.45] },
    "outCirc": { "$value": [0, 0.55, 0.45, 1] },
    "inOutCirc": {"$value": [0.85, 0, 0.15, 1] },
    "outCubic": { "$value": [0.33, 1, 0.68, 1] },
    "inCubic": { "$value": [0.32, 0, 0.67, 1] },
    "inOutCubic": { "$value": [0.65, 0, 0.35, 1] }
  }
}
