{
  "color": {
    "$type": "color",
    "blue": {
      "$value": "#218bff",
      "$extensions": {
        "mode": {
          "light": "#218bff",
          "light-colorblind": "#218bff",
          "light-high-contrast": "#1168e3",
          "dark": "#388bfd",
          "dark-dimmed": "#4184e4",
          "dark-high-contrast": "#409eff",
          "dark-colorblind": "#388bfd"
        }
      }
    },
    "action": {
      "$value": "{color.blue}",
      "$extensions": {
        "mode": {
          "light": "{color.blue#light}",
          "light-colorblind": "{color.blue#light-colorblind}",
          "light-high-contrast": "{color.blue#light-high-contrast}",
          "dark": "{color.blue#dark}",
          "dark-dimmed": "{color.blue#dark-dimmed}",
          "dark-high-contrast": "{color.blue#dark-high-contrast}",
          "dark-colorblind": "{color.blue#dark-colorblind}"
        }
      }
    }
  },
  "gradient": {
    "$type": "gradient",
    "$value": [
      {"color": "#218bff", "position": 0},
      {"color": "#e85aad", "position": 1}
    ]
  }
}
