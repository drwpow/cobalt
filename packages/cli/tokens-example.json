{
  "$name": "My Tokens",
  "$description": "Automatically generated with `co init`",
  "color": {
    "$type": "color",
    "$extensions": {
      "requiredModes": ["standard", "colorblind"]
    },
    "blue": {
      "$value": "#54aeff",
      "$extensions": {
        "mode": {
          "standard": "#54aeff",
          "colorblind": "#54aeff"
        }
      }
    },
    "green": {
      "$value": "#4ac26b",
      "$extensions": {
        "mode": {
          "standard": "#4ac26b",
          "colorblind": "#35adff"
        }
      }
    },
    "red": {
      "$value": "#fa4549",
      "$extensions": {
        "mode": {
          "standard": "#fa4549",
          "colorblind": "#d08002"
        }
      }
    }
  },
  "typography": {
    "family": {
      "$type": "fontFamily",
      "sans": {
        "$value": ["system-ui", "ui-sans-serif", "-apple-system", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"]
      },
      "monospace": {
        "$value": ["ui-monospace", "SF Mono", "Monaco", "Cascadia Mono", "Cascadia Code", "Consolas", "monospace"]
      }
    },
    "heading": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{typography.family.sans}",
        "fontSize": "36px",
        "fontWeight": 700,
        "lineHeight": 1.25,
        "letterSpacing": "-0.035em"
      }
    },
    "body": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{typography.family.sans}",
        "fontSize": "16px",
        "fontWeight": 400,
        "lineHeight": 1.5,
        "letterSpacing": 0
      }
    },
    "code": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{typography.family.monospace}",
        "fontSize": "14px",
        "fontWeight": 400,
        "lineHeight": 1.5,
        "letterSpacing": 0
      }
    }
  }
}
