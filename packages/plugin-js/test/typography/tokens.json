{
  "typography": {
    "family": {
      "body": { "$type": "fontFamily", "$value": ["IBM Plex Sans", "-system-ui", "sans-serif"] },
      "heading": { "$type": "fontFamily", "$value": "Helvetica" }
    },
    "largeTitle": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{typography.family.heading}",
        "fontSize": { "value": 34, "unit": "px" },
        "fontWeight": 400,
        "lineHeight": { "value": 41, "unit": "px" }
      },
      "$extensions": {
        "mode": {
          "xs": { "fontSize": { "value": 31, "unit": "px" }, "lineHeight": { "value": 38, "unit": "px" } },
          "s": { "fontSize": { "value": 32, "unit": "px" }, "lineHeight": { "value": 39, "unit": "px" } },
          "m": { "fontSize": { "value": 33, "unit": "px" }, "lineHeight": { "value": 40, "unit": "px" } },
          "l": { "fontSize": { "value": 34, "unit": "px" }, "lineHeight": { "value": 41, "unit": "px" } },
          "xl": { "fontSize": { "value": 36, "unit": "px" }, "lineHeight": { "value": 43, "unit": "px" } },
          "2xl": { "fontSize": { "value": 38, "unit": "px" }, "lineHeight": { "value": 46, "unit": "px" } },
          "3xl": { "fontSize": { "value": 40, "unit": "px" }, "lineHeight": { "value": 48, "unit": "px" } }
        }
      }
    },
    "body": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{typography.family.heading}",
        "fontSize": { "value": 17, "unit": "px" },
        "fontWeight": 400,
        "lineHeight": { "value": 22, "unit": "px" }
      },
      "$extensions": {
        "mode": {
          "xs": { "fontSize": { "value": 14, "unit": "px" }, "lineHeight": { "value": 19, "unit": "px" } },
          "s": { "fontSize": { "value": 15, "unit": "px" }, "lineHeight": { "value": 20, "unit": "px" } },
          "m": { "fontSize": { "value": 16, "unit": "px" }, "lineHeight": { "value": 21, "unit": "px" } },
          "l": { "fontSize": { "value": 17, "unit": "px" }, "lineHeight": { "value": 22, "unit": "px" } },
          "xl": { "fontSize": { "value": 19, "unit": "px" }, "lineHeight": { "value": 24, "unit": "px" } },
          "2xl": { "fontSize": { "value": 21, "unit": "px" }, "lineHeight": { "value": 26, "unit": "px" } },
          "3xl": { "fontSize": { "value": 23, "unit": "px" }, "lineHeight": { "value": 29, "unit": "px" } }
        }
      }
    }
  }
}
