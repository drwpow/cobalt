{
  "$name": "My Tokens",
  "$description": "Automatically generated with `co init`",
  "color": {
    "$extensions": {
      "requiredModes": ["standard", "colorblind"]
    },
    "blue": {
      "$type": "color",
      "$value": "#54aeff",
      "$extensions": {
        "mode": {
          "standard": "#54aeff",
          "colorblind": "#54aeff"
        }
      }
    },
    "green": {
      "$type": "color",
      "$value": "#4ac26b",
      "$extensions": {
        "mode": {
          "standard": "#4ac26b",
          "colorblind": "#35adff"
        }
      }
    },
    "red": {
      "$type": "color",
      "$value": "#fa4549",
      "$extensions": {
        "mode": {
          "standard": "#fa4549",
          "colorblind": "#d08002"
        }
      }
    }
  },
  "typography": {
    "family": {
      "$type": "fontFamily",
      "ibmPlex": {
        "$value": ["IBM Plex Sans", "-system-ui", "sans-serif"]
      }
    },
    "heading": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{typography.family.ibmPlex}",
        "fontSize": "36px",
        "fontWeight": 700,
        "lineHeight": 1.25,
        "letterSpacing": "-0.035em"
      }
    },
    "body": {
      "$type": "typography",
      "$value": {
        "fontFamily": "{typography.family.ibmPlex}",
        "fontSize": "16px",
        "fontWeight": 400,
        "lineHeight": 1.5,
        "letterSpacing": 0
      }
    }
  }
}
