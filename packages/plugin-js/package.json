{
  "name": "@cobalt-ui/plugin-js",
  "description": "Generate JS, TS, and JSON from your design tokens schema (requires @cobalt-ui/cli)",
  "version": "1.3.1",
  "author": {
    "name": "Drew Powers",
    "email": "drew@pow.rs"
  },
  "keywords": [
    "cobalt",
    "design tokens"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/drwpow/cobalt-ui.git",
    "directory": "packages/plugin-js"
  },
  "homepage": "https://cobalt-ui.pages.dev",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "run-s -s build:*",
    "build:clean": "del dist",
    "build:ts": "tsc",
    "build:license": "node ../../scripts/inject-license.js @cobalt-ui/plugin-js dist/index.js",
    "dev": "tsc -w",
    "pretest": "co build -c ./test/types.tokens.mjs",
    "test": "run-p -s test:*",
    "test:js": "vitest run",
    "test:ts": "tsc --noEmit"
  },
  "peerDependencies": {
    "@cobalt-ui/cli": "1.x"
  },
  "dependencies": {
    "@cobalt-ui/utils": "^1.2.2"
  },
  "devDependencies": {
    "@cobalt-ui/cli": "^1.5.0",
    "@cobalt-ui/core": "^1.4.3",
    "fast-glob": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "vitest": "^0.34.3"
  }
}
